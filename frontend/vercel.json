{
  "framework": "nextjs",
  "buildCommand": "next build",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://news-room.modal.run",
    "NEXT_PUBLIC_AUTH_DOMAIN": "news-room.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://news-room.modal.run/:path*"
    }
  ]
}

